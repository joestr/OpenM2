<?xml version="1.0" encoding="ISO-8859-1"?>
<IMPORT VERSION="1.0">
    <OBJECTS>
        <OBJECT TYPECODE="Include"
                TYPE="Inklude"
                MAYEXISTIN="ExportContainer"
                SHOWINMENU="YES"
                ISSEARCHABLE="NO"
                DBTABLE="dbm_include"
                CLASS="ibs.obj.incl.Include_01">
            <SYSTEM DISPLAY="YES">
                <NAME/>
                <DESCRIPTION/>
                <VALIDUNTIL/>
            </SYSTEM>
            <VALUES>
                <VALUE FIELD="topic"
                       TYPE="SELECTIONBOX"
                       OPTIONS="welcome"
                       MANDATORY="NO"
                       DBFIELD="m_topic">welcome</VALUE>

                <VALUE FIELD="content"
                       TYPE="HTMLTEXT"
                       MANDATORY="NO"
                       DBFIELD="m_content"/>

                <VALUE FIELD="priority"
                       TYPE="INTEGER"
                       MANDATORY="YES"
                       DBFIELD="m_priority"/>
            </VALUES>
        </OBJECT>
    </OBJECTS>
</IMPORT>
