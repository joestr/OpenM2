<?xml version="1.0" encoding="ISO-8859-1"?>
<IMPORT VERSION="1.0">
	<OBJECTS>
		<OBJECT TYPECODE="ExportInterface" TYPE="export interface">
			<SYSTEM>
				<ID DOMAIN=""/>
				<NAME>export interface</NAME>
				<DESCRIPTION/>
				<VALIDUNTIL/>
			</SYSTEM>
			<VALUES>
				<VALUE FIELD="connector" DBFIELD="m_connectorOid" TYPE="SELECTION" TYPECODEFILTER="FileConnector,FTPConnector,MailConnector,HTTPConnector,HTTPScriptConnector,EDISwitchConnector,SAPBCXMLRFCConnector" SEARCHROOT="group" SEARCHRECURSIVE="YES"/>
