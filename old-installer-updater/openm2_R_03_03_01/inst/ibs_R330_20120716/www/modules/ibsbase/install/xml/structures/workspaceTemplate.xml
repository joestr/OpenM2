<?xml version="1.0" encoding="ISO-8859-1"?>
<IMPORT VERSION="1.0">
	<OBJECTS>
		<OBJECT TYPECODE="Container">
			<SYSTEM>
                <ID DOMAIN="ibs_instobj" ADDWSPUSER="true">wsp_workbox</ID>
				<NAME>workbox</NAME>
				<DESCRIPTION>Your personal working area.</DESCRIPTION>
				<SHOWINNEWS>false</SHOWINNEWS>
			</SYSTEM>
			<RIGHTS>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="READ"/>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="WRITE"/>
			</RIGHTS>
			<OBJECTS>
				<OBJECT TYPECODE="Note">
					<SYSTEM>
                        <ID DOMAIN="ibs_instobj" ADDWSPUSER="true">wsp_workboxnote</ID>
						<NAME>Welcome</NAME>
						<SHOWINNEWS>false</SHOWINNEWS>
					</SYSTEM>
					<VALUES>
						<VALUE FIELD="content" TYPE="LONGTEXT">Welcome to the system.</VALUE>
					</VALUES>
				</OBJECT>
			</OBJECTS>
		</OBJECT>
