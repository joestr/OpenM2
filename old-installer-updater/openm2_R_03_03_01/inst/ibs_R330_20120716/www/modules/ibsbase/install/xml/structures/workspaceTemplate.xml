        <OBJECT TYPECODE="NewsContainer">
            <SYSTEM>
                <ID DOMAIN="ibs_instobj" ADDWSPUSER="true">wsp_news</ID>
                <NAME>news</NAME>
                <DESCRIPTION>Everthing, which has been added within a defined period (=&gt; user profile).</DESCRIPTION>
                <VALIDUNTIL>#CONFVAR.ibsbase.validUntil#</VALIDUNTIL>
                <SHOWINNEWS>false</SHOWINNEWS>
            </SYSTEM>
            <VALUES/>
			<RIGHTS>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="READ"/>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="WRITE"/>
			</RIGHTS>
        </OBJECT>
		<OBJECT TYPECODE="Inbox">
			<SYSTEM>
                <ID DOMAIN="ibs_instobj" ADDWSPUSER="true">wsp_inbox</ID>
				<NAME>inbox</NAME>
				<DESCRIPTION>Your personal inbox.</DESCRIPTION>
				<SHOWINNEWS>false</SHOWINNEWS>
			</SYSTEM>
			<RIGHTS>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="READ"/>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="WRITE"/>
			</RIGHTS>
		</OBJECT>
		<OBJECT TYPECODE="SentObjectContainer">
			<SYSTEM>
                <ID DOMAIN="ibs_instobj" ADDWSPUSER="true">wsp_outbox</ID>
				<NAME>outbox</NAME>
				<DESCRIPTION>Your personal outbox.</DESCRIPTION>
				<SHOWINNEWS>false</SHOWINNEWS>
			</SYSTEM>
			<RIGHTS>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="READ"/>
				<RIGHT NAME="#SYSVAR.WORKSPACEUSERNAME#" TYPE="USER" PROFILE="WRITE"/>
			</RIGHTS>
		</OBJECT>
        <OBJECT TYPECODE="QueryExecutive">
            <SYSTEM>
                <ID DOMAIN="ibs_instobj" ADDWSPUSER="true">wsp_checkoutobjects</ID>
                <NAME>checkout objects</NAME>
                <DESCRIPTION>Objects checked out by you.</DESCRIPTION>
                <VALIDUNTIL>#CONFVAR.ibsbase.validUntil#</VALIDUNTIL>
                <SHOWINNEWS>false</SHOWINNEWS>
            </SYSTEM>
            <VALUES>
                <VALUE FIELD="queryobjectname" TYPE="CHAR">ibs_getCheckOutObjects</VALUE>
            </VALUES>
        </OBJECT>
	</OBJECTS>
</IMPORT>
